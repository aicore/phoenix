{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "../src-desktop",
    "distDir": "../src-desktop",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "phoenix",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "phcode.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [
        "../src/",
        "../.brackets.json",
        "../.commitlintrc.json",
        "../.eslintignore",
        "../.eslintrc.js",
        "../CODE_OF_CONDUCT.md",
        "../CONTRIBUTING.md",
        "../ISSUE_TEMPLATE.md",
        "../LICENSE",
        "../NOTICE",
        "../package.json",
        "../package-lock.json",
        "../README.md",
        "../require.min.config.js",
        "../SECURITY.md"
      ],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "phoenix",
        "width": 800
      }
    ]
  }
}
